{
  "name": "@fsac/react-ui",
  "version": "0.1.5",
  "description": "Reusable React UI components",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "tailwind-preset.cjs"
  ],
  "module": "dist/react-ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/react-ui.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./tailwind-preset": {
      "require": "./tailwind-preset.cjs"
    }
  },
  "scripts": {
    "build": "bin/build.sh",
    "watch": "npx nodemon --watch ./src --exec bash --ext ts,tsx ./bin/build.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FullStackAppCo/react-ui.git"
  },
  "author": "Joby Harding",
  "license": "MIT",
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3"
  },
  "devDependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.12",
    "@types/react-fontawesome": "^1.6.5",
    "esbuild": "^0.14.43",
    "express": "^4.18.1",
    "fathom-client": "^3.4.1",
    "lodash-es": "^4.17.21",
    "nodemon": "^2.0.16",
    "pug": "^3.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3",
    "yargs": "^17.5.1"
  }
}
